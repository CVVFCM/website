{
    "name": "cvvfcm/site-v4",
    "license": "proprietary",
    "type": "project",
    "require": {
        "php": "^8.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-imagick": "*",
        "cmsig/seal": "^0.12.3",
        "cmsig/seal-loupe-adapter": "^0.12.2",
        "doctrine/doctrine-bundle": "^2.18.1",
        "doctrine/doctrine-fixtures-bundle": "^4.3.0",
        "doctrine/doctrine-migrations-bundle": "*",
        "friendsofsymfony/http-cache-bundle": "^3.0",
        "runtime/frankenphp-symfony": "^0.2.0",
        "scheb/2fa-bundle": "^7.11.0",
        "scheb/2fa-email": "^7.11.0",
        "scheb/2fa-trusted-device": "^7.11.0",
        "scienta/doctrine-json-functions": "^6.3",
        "stof/doctrine-extensions-bundle": "^1.14.0",
        "sulu/sulu": "^3.0.0-beta4",
        "symfony/asset": "7.3.*",
        "symfony/asset-mapper": "7.3.*",
        "symfony/config": "7.3.*",
        "symfony/dotenv": "7.3.*",
        "symfony/flex": "^2.9.0",
        "symfony/framework-bundle": "7.3.*",
        "symfony/mailer": "7.3.*",
        "symfony/monolog-bridge": "7.3.*",
        "symfony/monolog-bundle": "^3.10.0",
        "symfony/runtime": "7.3.*",
        "symfony/security-bundle": "7.3.*",
        "symfony/stimulus-bundle": "^2.31.0",
        "symfony/twig-bundle": "7.3.*",
        "symfony/ux-leaflet-map": "^2.31.0",
        "symfony/ux-map": "^2.21.0",
        "symfony/ux-twig-component": "^2.31.0",
        "twig/extra-bundle": "^3.22.0",
        "twig/intl-extra": "^3.22.0",
        "twig/twig": "^3.22.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.24",
        "friendsofphp/php-cs-fixer": "^3.89",
        "phpunit/phpunit": "^12.4.2",
        "symfony/browser-kit": "7.3.*",
        "symfony/css-selector": "7.3.*",
        "symfony/debug-bundle": "7.3.*",
        "symfony/error-handler": "7.3.*",
        "symfony/web-profiler-bundle": "7.3.*",
        "vimeo/psalm": "*",
        "vincentlanglet/twig-cs-fixer": "^3.10"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php84": "*",
        "symfony/polyfill-php83": "*",
        "symfony/polyfill-php82": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "bin/websiteconsole cache:clear": "php-script",
            "bin/adminconsole sulu:media:init": "php-script",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "reset": [
            "php bin/console doctrine:schema:drop --force --full-database",
            "php bin/console doctrine:schema:create",
            "php bin/console cmsig:seal:index-drop --force",
            "php bin/console cmsig:seal:index-create",
            "php bin/console sulu:security:role:create admin Sulu",
            "php bin/console sulu:security:user:create admin Adam Ministrator admin@example.com fr admin !ChangeMe!",
            "php bin/console sulu:security:init",
            "php bin/console sulu:page:initialize",
            "php bin/console doctrine:fixtures:load --no-interaction --append",
            "php bin/console cmsig:seal:reindex"
        ],
        "reset-test": [
            "php bin/console -etest doctrine:database:create --if-not-exists",
            "php bin/console -etest doctrine:schema:drop --force --full-database",
            "php bin/console -etest doctrine:schema:create",
            "php bin/console -etest cmsig:seal:index-drop --force",
            "php bin/console -etest cmsig:seal:index-create",
            "php bin/console -etest sulu:security:role:create admin Sulu",
            "php bin/console -etest sulu:security:user:create admin Adam Ministrator admin@example.com fr admin !ChangeMe!",
            "php bin/console -etest sulu:security:init",
            "php bin/console -etest sulu:page:initialize",
            "php bin/console -etest doctrine:fixtures:load --no-interaction --append",
            "php bin/console -etest cmsig:seal:reindex"
        ]
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "symfony/thanks": true,
            "phpstan/extension-installer": true
        },
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "7.3.*"
        },
        "runtime": {
            "class": "Runtime\\FrankenPhpSymfony\\Runtime"
        }
    }
}
